class Scene2(object):
    '''
    classdocs
    '''

    altdriver=None
    
    def __init__(self, altdriver):
        self.altdriver=altdriver
    
    def S1_flow_01(self):
        self.scene1=Scene1Tasks(self.altdriver)
        self.altdriver.wait_for_current_scene_to_be('MainMenu')
        sleep(10)
        self.scene1.checkInformation()
        self.scene1.clickPlay()