from time import sleep

class Scene2Tasks(object):
    '''
    classdocs
    '''
    altdriver=None
    
    def __init__(self, altdriver):
        self.altdriver=altdriver
        
    def clickLives(self):
        self.altdriver.wait_for_element('Base').tap()
        sleep(5)
        self.altdriver.wait_for_element('Close').tap()
        
    def clickCoins(self):
        self.altdriver.wait_for_element('Base').tap()
        sleep(5)
        self.altdriver.wait_for_element('Close').tap()
        
    def clickSettings:
        print 'hello'